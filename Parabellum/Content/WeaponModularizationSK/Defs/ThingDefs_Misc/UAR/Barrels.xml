	<!--6" Barrel-->
	<ThingDef ParentName="Module_Barrel">
		<defName>Module_6inchB</defName>
		<label>AR style 6" barrel</label>
		<description>6 inch pistol length barrel of AR family.</description>
		<statBases>
			<WorkToMake>400</WorkToMake>
			<Mass>0.388</Mass>
			<Bulk>0.4</Bulk>
		</statBases>
		<costList>
			<Weapon_Parts>1</Weapon_Parts>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<recipeMaker>
			<researchPrerequisite>Rifles_C1</researchPrerequisite>
			<skillRequirements>
				<Crafting>4</Crafting>
			</skillRequirements>
		</recipeMaker>
		<comps>
			<li Class="RW_ModularizationWeapon.CompProperties_ModularizationWeapon">
				<PartTexPath>Things/WeaponPart/Barrels/Pa_AR_Ba</PartTexPath>
				<drawChildPartWhenOnGround>false</drawChildPartWhenOnGround>
				<setRandomPartWhenCreate>true</setRandomPartWhenCreate>
				<statOffset>
					<Bulk>0.25</Bulk>
					<MarketValue>80</MarketValue>
				</statOffset>
				<verbPropertiesOffseter>
					<li>
						<range>8</range>
						<burstShotCount>7</burstShotCount>
					</li>
				</verbPropertiesOffseter>
				<verbPropertiesMultiplier>
					<li>
						<warmupTime>1.15</warmupTime>
						<ticksBetweenBurstShots>0.948</ticksBetweenBurstShots>
					</li>
				</verbPropertiesMultiplier>
				<statMultiplier>
					<SightsEfficiency>2</SightsEfficiency>
					<ShotSpread>0.25</ShotSpread>
					<RangedWeapon_Cooldown>1.1</RangedWeapon_Cooldown>
				</statMultiplier>
				<compPropertiesOffseter>
					<il Reader-Class="CombatExtended.CompProperties_FireModes">
						<aimedBurstShotCount>6</aimedBurstShotCount>
					</il>
				</compPropertiesOffseter>
				<attachmentProperties>
					<li>
						<id>ModAR_HG</id>
						<name>handguard</name>
						<UITexPath>ui/icons/AttachmentIcon/AttachmentIcon_Handguard</UITexPath>
						<postion>(0,0.1,0)</postion>
						<scale>(0.8,0,0.8)</scale>
						<allowEmpty>true</allowEmpty>
						<filter>
							<thingDefs>
								<li>Module_4inchHG</li>
								<li>Module_8inchHG</li>
								<li>Module_12inchHG</li>
							</thingDefs>
						</filter>
					</li>
					<li>
						<id>ModMP_Muzzle</id>
						<name>muzzle</name>
						<UITexPath>ui/icons/AttachmentIcon/AttachmentIcon_Muzzle</UITexPath>
						<postion>(-0.050535,0.1,0)</postion>
						<scale>(0.5,0,0.5)</scale>
						<allowEmpty>true</allowEmpty>
						<filter>
							<thingDefs>
								<li>Module_Silencer</li>
								<li>Module_MuzzleBrake</li>
								<li>Module_MuzzleCompensator</li>
								<li>Module_FlashHider</li>
							</thingDefs>
						</filter>
					</li>
				</attachmentProperties>
			</li>
		</comps>
	</ThingDef>
	
